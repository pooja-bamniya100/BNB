package com.bnb.model;

import com.sun.istack.NotNull;

import net.bytebuddy.implementation.auxiliary.AuxiliaryType.SignatureRelevant;

public class Users {
  private long id;
  private String name;
  @NotNull
  
  private String phone_number;
  private String password_digest;
	double wallet_balance;
	double avg_rating;
	String auth_id;
	String profile_image;
	String email;
	String pnb_user_id;
	int gender;
	int[] favourite_shops;
	public Users() {
		// TODO Auto-generated constructor stub
	}

}

//avg_rating double precision,

pincode character varying(6) COLLATE pg_catalog."default",
latitude character varying COLLATE pg_catalog."default",
longitude character varying COLLATE pg_catalog."default",
terms_conditions boolean DEFAULT false,
whatsapp_subscription boolean DEFAULT false,
registered boolean DEFAULT false,
referral_code character varying COLLATE pg_catalog."default",
referral_link character varying COLLATE pg_catalog."default",
date_of_birth date,
referral_code_used boolean,
CONSTRAINT users_pkey PRIMARY KEY (id)